{
  "name": "@lighting-beetle/lighter-styleguide",
  "version": "6.0.0",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.module.js",
  "jsnext:main": "dist/index.module.js",
  "exports": {
    ".": "./dist/index.module.js"
  },
  "files": [
    "dist",
    "public"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "next dev",
    "build": "run-s clean 'build:*'",
    "clean": "rimraf dist out",
    "build:components": "node ./esbuild.components.js",
    "build:types": "tsc --declaration  --emitDeclarationOnly --noEmit false --outDir dist -p ./tsconfig.json",
    "build:styleguide": "next build && next export"
  },
  "dependencies": {
    "@babel/runtime": "~7.15.4",
    "@mdx-js/react": "^1.6.22",
    "@types/react-table": "^7.7.5",
    "classnames": "~2.3.1",
    "gray-matter": "~4.0.3",
    "modern-normalize": "^1.1.0",
    "next": "~12.2.5",
    "polished": "^4.1.3",
    "pretty": "~2.0.0",
    "prism-react-renderer": "~1.2.1",
    "react": "~17.0.2",
    "react-dom": "~17.0.2",
    "react-element-to-jsx-string": "~14.3.4",
    "react-table": "^7.7.0",
    "styled-components": "~5.3.1",
    "unescape-html": "~1.1.0"
  },
  "devDependencies": {
    "@esbuild-plugins/esm-externals": "0.0.6",
    "@lighting-beetle/next-lighter-config": "workspace:^",
    "@mdx-js/loader": "^1.6.22",
    "@types/styled-components": "^5.1.14",
    "esbuild": "^0.13.3",
    "esbuild-plugin-import-glob": "^0.1.1",
    "esbuild-plugin-svg": "~0.1.0",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2",
    "typescript": "^4.9.3"
  },
  "peerDependencies": {
    "react": ">=17.0.2"
  }
}
