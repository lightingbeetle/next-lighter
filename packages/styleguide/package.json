{
  "name": "@lighting-beetle/lighter-styleguide",
  "version": "5.0.0-alpha.1",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.modern.js",
  "files": [
    "dist",
    "public"
  ],
  "types": "./dist/index.d.ts",
  "scripts": {
    "dev": "next dev",
    "build": "npm-run-all build:*",
